{
  "schemaVersion": 1,
  "id": "mcpitanlib",
  "version": "${version}",
  "name": "MCPitanLib",
  "description": "Pitan's Minecraft Mod Library",
  "authors": [
    "Pitan"
  ],
  "contact": {
    "website": "https://www.curseforge.com/minecraft/mc-mods/mcpitanlibarch",
    "repo": "https://github.com/Pitan76/mcpitanlibarch",
    "issues": "https://github.com/Pitan76/mcpitanlibarch/issue"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.pitan76.mcpitanlib.fabric.MCPitanLibFabric"
    ]
  },
  "provides": [
    "mcpitanlibarch",
    "mcpitanlib-impl"
  ],
  "custom": {
    "modmenu": {
      "badges": ["library"]
    }
  },
  "mixins": [
    "mcpitanlib.mixins.json",
    "mcpitanlib-common.mixins.json"
  ],
  "accessWidener" : "mcpitanlib.accesswidener",
  "depends": {
    "fabricloader": "*",
    "minecraft": ">=1.19"
  }
}
